<div class="p3-container">
  <div class="disp-container">
    <div class="disp-card">
      <h2>Synth A</h2>
      <div class="info">
        <div class="info-key">
          <h3>Pitch </h3>
          <h3>Frequency </h3>
          <h3>Volume </h3>
        </div>
        <div class="info-val">
          <span id="cur-pitch-3A"></span>
          <span id="cur-freq-3A"></span>
          <span id="cur-vol-3A"></span>
        </div>
      </div>
    </div>
    <div class="disp-card">
      <h2>Harmonic Interval</h2>
      <div class="info-row">
        <div class="info-col">
          <h3>Name </h3>
          <span id="cur-int"></span>
        </div>
        <div class="info-col">
          <h3>Abbreviation </h3>
          <span id="cur-int-abbr" class="unruly-abbr"></span>
        </div>
        <div class="info-col">
          <h3># Half-Steps </h3>
          <span id="cur-int-hs"></span>
        </div>
      </div>
    </div>
    <div class="disp-card">
      <h2>Synth B</h2>
      <div class="info">
        <div class="info-key">
          <h3>Pitch </h3>
          <h3>Frequency </h3>
          <h3>Volume </h3>
        </div>
        <div class="info-val">
          <span id="cur-pitch-3B"></span>
          <span id="cur-freq-3B"></span>
          <span id="cur-vol-3B"></span>
        </div>
      </div>
    </div>
  </div>

  <div class="synth-rack">
    <div class="synth-card">
      <div class="synth-header">
        <div>
          <h2>Synth A</h2>
        </div>
        <div>
          <label for="synth-list"><h3>Synth Type:</h3></label>
          <select name="synth-list" id="synth-list-3a">
            <option>AM Synth</option>
            <option>FM Synth</option>
            <option>Mono Synth</option>
            <option>Simple Synth</option>
          </select>
        </div>
      </div>
      <div class="control-panel">
        <div class="control-mod">
          <h3>Volume A(decibels)</h3>
          <div class="slider" id="slider-v-3a">
            <div id="vol-hand-3a" class="v-sli sli-han ui-slider-handle"></div>
          </div>
        </div>
        <div class="control-mod">
          <h3>Equalizer A(Low-Mid-High)</h3>
          <div class="eq-fader">
            <div id="slider-eq-low-3a">
              <div
                id="hand-eq-l-3a"
                class="v-sli sli-han ui-slider-handle"
              ></div>
            </div>
            <div id="slider-eq-mid-3a">
              <div
                id="hand-eq-m-3a"
                class="v-sli sli-han ui-slider-handle"
              ></div>
            </div>
            <div id="slider-eq-high-3a">
              <div
                id="hand-eq-h-3a"
                class="v-sli sli-han ui-slider-handle"
              ></div>
            </div>
          </div>
        </div>
      </div>
    </div>

    <div class="synth-card">
      <div class="synth-header">
        <div>
          <h2>Synth B</h2>
        </div>
        <div>
          <label for="synth-list"><h3>Synth Type:</h3></label>
          <select name="synth-list" id="synth-list-3b">
            <option>AM Synth</option>
            <option>FM Synth</option>
            <option>Mono Synth</option>
            <option>Simple Synth</option>
          </select>
        </div>
      </div>
      <div class="control-panel">
        <div class="control-mod">
          <h3>Volume B(decibels)</h3>
          <div class="slider" id="slider-v-3b">
            <div id="vol-hand-3b" class="v-sli sli-han ui-slider-handle"></div>
          </div>
        </div>
        <div class="control-mod">
          <h3>Equalizer B(Low-Mid-High)</h3>
          <div class="eq-fader">
            <div id="slider-eq-low-3b">
              <div
                id="hand-eq-l-3b"
                class="v-sli sli-han ui-slider-handle"
              ></div>
            </div>
            <div id="slider-eq-mid-3b">
              <div
                id="hand-eq-m-3b"
                class="v-sli sli-han ui-slider-handle"
              ></div>
            </div>
            <div id="slider-eq-high-3b">
              <div
                id="hand-eq-h-3b"
                class="v-sli sli-han ui-slider-handle"
              ></div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
