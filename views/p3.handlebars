  <div class="note-disp-container">
    <div class="disp-card a">
      <h2>Note<span class="symbol" id="note-3A"></span></h2>
    </div>

    <div class="disp-card b">
      <h2>Note<span class="symbol" id="note-3B"></span></h2>
    </div>

    <div class="disp-card c">
      <h2>Note<span class="symbol" id="note-3C"></span></h2>
    </div>
  </div>
  <div class="int-disp-row">
    <div class="disp-card ab">
      <h2>Interval<span class="symbol" id="int-1"></span></h2>
    </div>

    <div class="disp-card bc">
      <h2>Interval<span class="symbol" id="int-2"></span></h2>
    </div>
  </div>
  <div class="chord-disp-row">
    <div class="disp-card abc">
      <h2>Chord<span class="symbol" id="chord-1"></span></h2>
    </div>
    <div id="alt-chord" class="disp-card abc-2" style="display:none">
      <h2><span class="symbol" id="chord-2"></span></h2>
    </div>
  </div>

  <div id="im-tog-div"><button id="img-tog">Change Keyboard Setup</button></div>
  <div id="show-img-div"><button id="img-disp-tog">Show Keyboard Map</button></div>
  <div class="kbrd-map">

    <img id="key-map" src="assets/images/audiokeys-1row.jpeg" />
  </div>
</div>