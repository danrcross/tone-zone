<div class="p3-container">
  <div id="sy-tog-cntr">
    <button class="ctrl-toggle">Open Synth Controls</button>
  </div>
  <div class="synth-rack">
    <div class="synth-card a">
      <div class="synth-header">
        <div>
          <h2></h2>
        </div>
        <div>
          <label for="synth-list"><h3>Synth Type:</h3></label>
          <select class="synth-dd" name="synth-list" id="synth-list-3a">
            <div class="opt-div">
              <option>AM Synth</option>
              <option>FM Synth</option>
              <option>Mono Synth</option>
              <option>Simple Synth</option>
            </div>
          </select>
        </div>
      </div>

      <div class="control-panel">
        <div class="mod-container">
          <div class="control-mod">
            <h3>Volume A(decibels)</h3>
            <div class="slider" id="slider-v-3a">
              <div
                id="vol-hand-3a"
                class="v-sli sli-han ui-slider-handle"
              ></div>
            </div>
          </div>
          <div class="control-mod">
            <h3>Equalizer A(Low-Mid-High)</h3>
            <div class="eq-fader">
              <div class="slider" id="slider-eq-low-3a">
                <div
                  id="hand-eq-l-3a"
                  class="v-sli sli-han ui-slider-handle"
                ></div>
              </div>
              <div class="slider" id="slider-eq-mid-3a">
                <div
                  id="hand-eq-m-3a"
                  class="v-sli sli-han ui-slider-handle"
                ></div>
              </div>
              <div class="slider" id="slider-eq-high-3a">
                <div
                  id="hand-eq-h-3a"
                  class="v-sli sli-han ui-slider-handle"
                ></div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>

